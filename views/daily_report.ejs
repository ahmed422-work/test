<!DOCTYPE html>
<html lang="en" class="h-100">


<head>
    <!-- meta and links start -->
    <%- include('templet/meta')-%>

</head>

<body>
    <%- include('templet/preloder_topbar_sidebar',{titel:"Report"})-%>

        <!--********** Content body start **********-->

        <div class="content-body">

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card ">
                            <div class="card-header">
                                <h3 class="card-title">Daily Report</h3>
                            </div>
                            <div class="d-none">
                                <input type="hiden" name="sym" value="<%= accessdata.symbol %>">
                                <input type="hiden" name="pls" value="<%= accessdata.plac %>">
                            </div>
                            <div class="card-body">
                                <div class="row d-flex justify-content-between">
                                    <div class="col-5">
                                        <label class="form-label">Date</label>
                                        <input type="date" class="form-control" name="date" id="daily_report_date" value="<%= date.toLocaleDateString('en-CA') %>">
                                    </div>
                                    <div class="form-group col-5 <%= accessdata.logas == 'master' && accessdata.mutibranch  ? '' :'d-none' %>">
                                        <label class="form-label">Store Name</label>
                                        <select class="default-select form-control wide mb-3" name="storeid"
                                            id="daily_report_store" >
                                            <% storeList.forEach(function(x){ %>
                                                <option value="<%= x.id %>" <%= x.id == store ? 'selected' :'' %> ><%= x.name %></option>
                                            <% }) %>
                                        </select>

                                    </div>
                                </div>

                              

                                <div class="row mt-4">

                                    <div class="table-responsive">
                                        <table class="table">
                                            <thead style="background-color: #D8D8D8; color: #1C1C1C;">
                                                <tr>
                                                    <th>Particulars</th>
                                                    <th>Value</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td> Orders </td>
                                                    <td style="color: #FFBF00; font-weight: 600;" id="daily_report_orders">
                                                        <%=orders %> 
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td> No. of Orders Delivered </td>
                                                    <td style="color: #0000FF; font-weight: 600;" id="daily_report_ordersdelivery">
                                                        <%=ordersdeliver %> 
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td> Total Sales </td>
                                                    <td style="color: #04B404; font-weight: 600;"  class="symbol" id="daily_report_totalsale">
                                                        <%=orderstotalseal.toFixed(2) %> 
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td> Total Payment </td>
                                                    <td style="color: #01A9DB; font-weight: 600;"  class="symbol" id="daily_report_payment">
                                                        <%=totalpay.toFixed(2) %> 
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td> Total Expense </td>
                                                    <td style="color: #DF013A; font-weight: 600;" class="symbol" id="daily_report_expence">
                                                        <%=expence.toFixed(2) %> 
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>

        <!--********** Content body end **********-->

        </div>
        <!--**********************************
            Footer start
        ***********************************-->
        <div class="footer">
            <div class="copyright">
                <p>Copyright Â© Designed &amp; Developed by <a href="#" target="_blank">KMS</a> 2023</p>
            </div>
        </div>
        <!--**********************************
            Footer end
        ***********************************-->

        </div>

        <!--********** Main wrapper end **********-->
        <%- include('templet/script')-%>
            

            

</body>


</html>