<!DOCTYPE html>
<html lang="en" class="h-100">


<head>
    <!-- meta and links start -->
    <%- include('templet/meta')-%>

</head>

<body>
    <%- include('templet/preloder_topbar_sidebar',{titel:"Pay Out"})-%>

        <!--********** Content body start **********-->

        <div class="content-body">

            <div class="container-fluid ">
                <div class="row">
                    <div class="d-none">
                        <input type="hiden" name="sym" value="<%= accessdata.symbol %>">
                        <input type="hiden" name="pls" value="<%= accessdata.plac %>">
                    </div>
                    <div class="col-12">
                        <div class="card mb-3">
                            <div class="card-body">
                                <div class="d-flex justify-content-center text-center">
                                    <div class="col-2 mx-3">
                                        <h3><span><%=comission %></span> $</h3>
                                        <h5>Total Pay Out Amount</h5>
                                    </div>
                                    <div class="col-2 mx-3">
                                        <h3><span><%=paidcomission %></span> $</h3>
                                        <h5>Total Received Amount</h5>
                                    </div>
                                    <div class="col-2 mx-3">
                                        <h3><span><%=due.toFixed(2) %></span> $</h3>
                                        <h5>Total Due Amount</h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-12">
                        <div class="card ">
                            <div class="card-header">
                                <h3 class="card-title">Store List</h3>
                            </div>

                            <div class="card-body">
                                <div class="table-responsive">
                                    <table
                                        class="table table-bordered table-striped verticle-middle table-responsive-sm">
                                        <thead>
                                            <tr>
                                                <th scope="col">#</th>
                                                <th scope="col">Store Name</th>
                                                <th scope="col">Payout Info</th>
                                                <th scope="col">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                           <% storeList.forEach(function(data, i){ %>

                                          
                                            <tr >
                                                <td><%= i + 1 %></td>
                                                <td>
                                                    <div> <b><%= data.name %></b> </div>
                                                    
                                                </td>
                                                <td>
                                                    <div> Total commission from store :- <b class="symbol"><%= data.comiision.toFixed(2) %> </b> </div>
                                                    <div> Store paid commission amount :- <b class="symbol"><%= data.pay.toFixed(2) %></b> </div>
                                                    <div> Due commission amount :- <b class="symbol"><%= (data.comiision -data.pay).toFixed(2) %></b> </div>
                                                </td>
                                                <td>
                                                    <a href="/account/payoutdetails/<%= data.id %>" class="btn btn-primary shadow sharp"><i class="flaticon-381-view"></i></a>
                                                </td>
                                            </tr>
                                        <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>

        <!--********** Content body end **********-->

        </div>
        <!--**********************************
            Footer start
        ***********************************-->
        <div class="footer">
            <div class="copyright">
                <p>Copyright Â© Designed &amp; Developed by <a href="#" target="_blank">KMS</a> 2023</p>
            </div>
        </div>
        <!--**********************************
            Footer end
        ***********************************-->

        </div>

        <!--********** Main wrapper end **********-->
        <%- include('templet/script')-%>
            

</body>



</html>